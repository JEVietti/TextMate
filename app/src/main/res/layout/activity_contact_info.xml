<AbsoluteLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.textmate.ContactInfoActivity"
    android:background="@drawable/abc_cab_background_top_holo_dark">

    <TextView
        android:id="@+id/myImageViewText"
        android:layout_width="141dp"
        android:layout_height="144dp"
        android:layout_alignLeft="@+id/myImageView"
        android:layout_alignTop="@+id/myImageView"
        android:layout_alignRight="@+id/myImageView"
        android:layout_alignBottom="@+id/myImageView"
        android:layout_margin="1dp"
        android:gravity="center"
        android:text="7"
        android:textColor="#FFFFFF"
        android:layout_x="-6dp"
        android:layout_y="3dp"
        android:textSize="@android:dimen/notification_large_icon_height"
        android:background="@drawable/abc_ab_bottom_solid_light_holo" />

    <TextView
        android:layout_width="140dp"
        android:layout_height="38dp"
        android:text="Mike Wazowski"
        android:id="@+id/contactName"
        android:layout_x="178dp"
        android:layout_y="25dp"
        android:textSize="@dimen/abc_action_bar_title_text_size"
        android:textColor="#ffffff"
        android:textIsSelectable="false" />
